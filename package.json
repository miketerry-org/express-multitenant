{
  "name": "express-tenants",
  "version": "0.11.1",
  "description": "Multitenant support for express applications",
  "main": "index.js",
  "scripts": {
    "dev": "NODE_ENV=DEV node --watch index.js",
    "encrypt": "topsecret encrypt .tenants.json .tenants.secure --key-file .tenants.key",
    "generate": "topsecret generate --key-file .tenants.key",
    "version": "topsecret --version",
    "test": "node --watch --test"
  },
  "dependencies": {
    "confirm-json": "^2.0.3",
    "topsecret": "^0.7.4"
  },
  "devDependencies": {
    "mongoose": "^8.13.2",
    "nodemailer": "^6.10.0",
    "winston": "^3.17.0",
    "winston-mongodb": "^6.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/miketerry-org/express-tenants.git"
  },
  "keywords": [
    "express",
    "database",
    "multitenant"
  ],
  "author": "miketerry.org",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/miketerry-org/express-tenants/issues"
  },
  "homepage": "https://github.com/miketerry-org/express-tenants#readme"
}
